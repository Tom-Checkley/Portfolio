{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "3f3cb46b-cb8d-4e02-ac15-f1dca3c69053",
        "SanitizeTinyMce": true
      },
      "ModelsBuilder": {
        "Enable": false,
        "ModelsMode": "Nothing"
      },
      "Tours": {
        "EnableTours": false
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=TomCheckley;Database=Portfolio;User Id=sa;Password=password123",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
  }
}